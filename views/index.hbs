<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>My first view</title>
    <link rel="stylesheet" href="stylesheets/style.css" />
  </head>
  <body>
    <h1>Ironhacker Be Like!</h1>
    {{#if isAllowed}}
      <p> {{name}} </p>
      <p> {{{bootcamp}}} </p>
    {{else}}
      <p>you are not allowed to see this content!</p>
    {{/if}}
    {{#unless address}}
      <h3>WARNING: We can't find the address!</h3>
    {{/unless}}
    {{#each cities}}
      <li>{{@index}}:{{this}}</li>
    {{/each}}
    {{#each objExample}}
      <div>{{@key}}:{{this}}</div>
    {{/each}}
    <img src="https://media.giphy.com/media/l0MYEqEzwMWFCg8rm/giphy.gif" />
  </body>
</html>